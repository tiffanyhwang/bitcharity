<%= @button.embed_html.html_safe %>
<div class="row">
  <% @charities.each do |c| %>
    <div class="col-sm-5 col-md-3">
      <div class="thumbnail">
        <%= image_tag c.logo %>
        <div class="caption">
          <h3><%= c.name %></h3>
          <p><%= c.description %></p>
          <p><a href="#" class="btn btn-primary" role="button">Pick <%= c.name %></a>
            <a href="<%= c.url %>" class="btn btn-default" role="button">Learn More</a></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
